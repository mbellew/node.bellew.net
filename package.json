{
  "name": "node.bellew.net",
  "version": "0.0.1",
  "config": {
    "eb": "/opt/local/Library/Frameworks/Python.framework/Versions/2.7/bin/eb",
    "rt": "nodejs ./node_modules/react-templates/bin/rt.js",
    "tsc": "./node_modules/bin/tsc --jsx react"
  },
  "scripts": {
    "start": "export PORT=80 && nodejs server",
    "build:js": "$npm_package_config_tsc static/bellew.ts -out static/bellew.js && $npm_package_config_rt static/templates/*.rt && cat static/templates/*.rt.js >> static/bellew.js",
    "build:server": "$npm_package_config_tsc server/server.ts -out server.js",
    "build": "npm run build:js && npm run build:server",
    "prerun": "npm run build",
    "run": "nodejs server",
    "predeploy": "npm run build",
    "deploy": "$npm_package_config_eb deploy"
  },
  "dependencies": {
    "finalhandler": "latest",
    "marked": "latest",
    "react": "latest",
    "react-dom": "latest",
    "serve-static": "latest",
    "typescript": "1.8.9",
    "watch": "latest"
  }
}
