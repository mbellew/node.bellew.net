    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve-static": "latest",
    "underscore": "^1.8.3",
    "watch": "latest"
  },
  "devDependencies": {
    "@types/jquery": "^3.2.17",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "awesome-typescript-loader": "^3.4.1",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.6.2"
  }
